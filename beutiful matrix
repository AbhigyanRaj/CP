#include <iostream>
using namespace std ;
       
int main(){
    
        int count;
        count = 0;
        int arr[5][5];
 
       
        for(int i = 0 ; i < 5 ; i++){
            for(int j = 0 ; j <5 ; j++){
               cin>>arr[i][j];
            }
            cout<<endl;

        
    }

    for(int i = 0 ; i < 5 ; i++){
            for(int j = 0 ; j <5 ; j++){
               if(arr[i][j]==1){
                count += i-1;
                count += j-1;
               }
               else{
                count += 0;
               }
            }
            cout<<endl;


        }

    
    cout<<count;
     
    

    
     
return 0;
     
}
